{
    "name": "PieFed",
    "short_name": "PieFed",
    "icons": [
        {
            "src": "/static/images/piefed_logo_icon_t_192.png",
            "sizes": "192x192",
            "type": "image/png",
            "purpose": "any maskable"
        },
        {
            "src": "/static/images/piefed_logo_icon_t_512.png",
            "sizes": "512x512",
            "type": "image/png",
            "purpose": "any maskable"
        }
    ],
    "theme_color": "#c6e2ff",
    "background_color": "#c6e2ff",
    "display": "standalone",
    "display_override": ["window-controls-overlay"],
    "orientation": "any",
    "start_url": "/",
    "scope": "/",
    "piefed_manifest_version": "default",
    "serviceworker": {
      "src": "/service_worker.js",
      "scope": "/",
      "use_cache": true
    },
    "share_target": {
        "action": "/share",
        "method": "GET",
        "enctype": "application/x-www-form-urlencoded",
        "params": {
          "title": "title",
          "url": "url"
        }
    },
    "shortcuts": [
      {
        "name": "Topics",
        "url": "/topics"
      },
      {
        "name": "Feeds",
        "url": "/feeds"
      },
      {
        "name": "Notifications",
        "url": "/notifications"
      },
      {
        "name": "Donate",
        "url": "/donate"
      }
    ],
    "prefer_related_applications": false
}
