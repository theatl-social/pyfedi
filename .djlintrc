{
    "profile": "jinja",
    "indent": 4,
    "max_line_length": 120,
    "format_css": false,
    "format_js": false,
    "ignore": "H005,H006,H007,H013,H014,H016,H017,H020,H021,H023,H025,H026,H029,H030,H031,H037,J004,J018,T001,T002,T003,T027,T028,T032",
    "custom_blocks": "render_form,render_field,render_topic",
    "custom_html": "fe,x-*",
    "exclude": ".git,__pycache__,.venv,venv,migrations",
    "extension": "html",
    "linter_output_format": "{filename}:{line}: {code} {message} {match}"
}